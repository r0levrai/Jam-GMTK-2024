.basic-button {
    -unity-font: url("project://database/Assets/Font/NanumPenScript-Regular.ttf?fileID=12800000&guid=d341cf114415f8a41b0a11a54f6fed77&type=3#NanumPenScript-Regular");
    font-size: 100px;
    -unity-text-align: middle-center;
    background-color: rgba(254, 255, 199, 0);
    background-image: url("project://database/Assets/Texture/Submit.png?fileID=2800000&guid=030ec949411adb54295d55c6ac14b00f&type=3#Submit");
    -unity-background-scale-mode: stretch-to-fill;
    -unity-font-definition: url("project://database/Assets/Font/NanumPenScript-Regular%20SDF%201.asset?fileID=11400000&guid=6a1362e9f2196ac4c9fa0ecb2c4b1338&type=2#NanumPenScript-Regular SDF 1");
    width: 365px;
    height: 115px;
    rotate: -1deg;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    color: rgb(27, 27, 27);
}

.basic-button:hover {
    background-color: rgba(0, 0, 0, 0);
    scale: 1.1 1.1;
    rotate: 3deg;
}

.basic-button:active {
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(253, 211, 147);
    scale: 1 1;
    rotate: -2deg;
}

.tool-selector-button {
    font-size: 100px;
    rotate: -12.5deg;
    translate: 0 0;
    display: flex;
    height: 160px;
    width: 1000px;
    color: rgba(27, 27, 27, 0);
    margin-top: -50px;
    left: -400px;
}

.tool-selector-button:hover {
    translate: 40px -8.87px;
}

.tool-selector-button:checked {
    translate: 130px -28.82px;
    color: rgba(27, 27, 27, 0);
}

.tool-selector-button:focus {
    background-color: rgba(0, 0, 0, 0);
}

.unity-radio-button__input {
    display: none;
    opacity: 1;
    visibility: hidden;
}

.redo-button {
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    rotate: -12.5deg;
}

.redo-button:hover {
    translate: 40px -8.87px;
}

.redo-button:active {
    rotate: -10deg;
    scale: 0.9 0.9;
}

.size-selector-button {
    width: 75px;
    height: 75px;
    margin-right: 0;
    margin-left: 30px;
}

.size-selector-button.small-brush {
    background-image: url("project://database/Assets/Texture/small_brush_icon.png?fileID=2800000&guid=1f45d7d1da729154cad60c4f04338e94&type=3#small_brush_icon");
}

.size-selector-button.medium-brush {
    background-image: url("project://database/Assets/Texture/medium_brush_icon.png?fileID=2800000&guid=de96dc06b99fb094b8c957f62b6f1127&type=3#medium_brush_icon");
}

.size-selector-button.big-brush {
    background-image: url("project://database/Assets/Texture/big_brush_icon.png?fileID=2800000&guid=cdd335a8bc036c64b8ad861dc20a39cb&type=3#big_brush_icon");
}

.size-selector-button:hover {
    scale: 1.1 1.1;
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

.size-selector-button.small-brush:checked {
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-image: url("project://database/Assets/Texture/small_brush_checked.png?fileID=2800000&guid=12eb8863d557b734fade766231403dba&type=3#small_brush_checked");
}

.size-selector-button.big-brush:checked {
    background-image: url("project://database/Assets/Texture/big_brush_checked.png?fileID=2800000&guid=2a62176f025d10f41b689a746969a7c6&type=3#big_brush_checked");
}

.size-selector-button.medium-brush:checked {
    background-image: url("project://database/Assets/Texture/medium_brush_checked.png?fileID=2800000&guid=ecfa100bf65f92c409bac88369f2c251&type=3#medium_brush_checked");
}

.unity-base-slider__dragger {
    background-image: url("project://database/Assets/Sprites/ui%20stuff%20layers.png?fileID=1691425476&guid=19a7656f5aba5e84680f1d932be6c644&type=3#ui stuff layers_14");
    width: 80%;
    right: -5px;
    left: auto;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(0, 0, 0, 0);
    height: 15px;
}

.unity-base-slider__tracker {
    background-image: url("project://database/Assets/Sprites/ui%20stuff%20layers.png?fileID=1044022483&guid=19a7656f5aba5e84680f1d932be6c644&type=3#ui stuff layers_9");
    width: 100%;
    background-color: rgba(0, 0, 0, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    left: 0;
    flex-grow: 1;
}
